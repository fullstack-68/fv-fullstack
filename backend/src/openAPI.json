{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "My API",
    "description": "This is the API."
  },
  "servers": [
    {
      "url": "/"
    }
  ],
  "components": {
    "schemas": {},
    "parameters": {}
  },
  "paths": {
    "/users": {
      "get": {
        "description": "Get all users",
        "summary": "Get all users",
        "tags": [
          "Right"
        ],
        "responses": {
          "200": {
            "description": "User data array",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "minLength": 1
                      },
                      "createdAt": {
                        "type": "number"
                      },
                      "firstName": {
                        "type": "string",
                        "minLength": 1
                      },
                      "lastName": {
                        "type": "string",
                        "minLength": 1
                      },
                      "email": {
                        "type": "string",
                        "format": "email"
                      },
                      "dateOfBirth": {
                        "type": "string",
                        "minLength": 1,
                        "pattern": "^\\d{4}\\-(0[1-9]|1[012])\\-(0[1-9]|[12][0-9]|3[01])$"
                      }
                    },
                    "required": [
                      "id",
                      "createdAt",
                      "firstName",
                      "lastName",
                      "email",
                      "dateOfBirth"
                    ]
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "description": "Create user",
        "summary": "Create user",
        "tags": [
          "Right"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "firstName": {
                    "type": "string",
                    "minLength": 1
                  },
                  "lastName": {
                    "type": "string",
                    "minLength": 1
                  },
                  "email": {
                    "type": "string",
                    "format": "email"
                  },
                  "dateOfBirth": {
                    "type": "string",
                    "minLength": 1,
                    "pattern": "^\\d{4}\\-(0[1-9]|1[012])\\-(0[1-9]|[12][0-9]|3[01])$"
                  },
                  "password": {
                    "type": "string",
                    "minLength": 4
                  },
                  "confirmPassword": {
                    "type": "string",
                    "minLength": 1
                  }
                },
                "required": [
                  "firstName",
                  "lastName",
                  "email",
                  "dateOfBirth",
                  "password",
                  "confirmPassword"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Reset sucessfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "status"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/users_wrong": {
      "get": {
        "description": "Get all users",
        "summary": "Get all users",
        "tags": [
          "Wrong"
        ],
        "responses": {
          "200": {
            "description": "User data array",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "minLength": 1
                      },
                      "createdAt": {
                        "type": "number"
                      },
                      "email": {
                        "type": "string",
                        "format": "email"
                      },
                      "firstname": {
                        "type": "string"
                      },
                      "lastname": {
                        "type": "string"
                      },
                      "dateOfBirth": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "id",
                      "createdAt",
                      "email",
                      "firstname",
                      "lastname",
                      "dateOfBirth"
                    ]
                  }
                }
              }
            }
          }
        }
      }
    },
    "/users/reset": {
      "post": {
        "description": "Reset users to initial data",
        "summary": "Reset users to initial data",
        "tags": [
          "Utility"
        ],
        "responses": {
          "200": {
            "description": "Reset sucessfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "status"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}